{
  "name": "@ulisesantana/cli-timer",
  "description": "Run a timer from your shell.",
  "version": "0.2.0",
  "author": "Ulises Santana @ulisesantana",
  "bin": {
    "timmy": "./bin/run"
  },
  "bugs": "https://github.com/ulisesantana/cli-timer/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "node-notifier": "^10.0.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^10.17.60",
    "@types/node-notifier": "^8.0.1",
    "after": "^0.8.2",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "jest": "^27.2.5",
    "ts-jest": "^27.0.7",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ulisesantana/cli-timer",
  "keywords": [
    "oclif",
    "cli",
    "timer",
    "background timer",
    "notification"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "timmy",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ulisesantana/cli-timer",
  "scripts": {
    "build": "tsc",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "prepublishOnly": "npm run tag",
    "tag": "TAG=v$(node -pe \"require('./package.json').version\"); git commit -am \"ðŸ”– $TAG\" && git tag $TAG && git push --tags",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
