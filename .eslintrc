{
  "env": {
    "node": true,
    "jest": true
  },
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    "no-process-exit": "off",
    "unicorn/no-process-exit": "off",
    "no-unused-vars": "off",
    "no-useless-constructor": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ]
  }
}